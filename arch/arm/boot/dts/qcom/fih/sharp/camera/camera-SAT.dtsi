		gpios = <&tlmm 32 0>,
			<&tlmm 46 0>,
			<&tlmm 13 0>,
			<&tlmm 50 0>;
		qcom,gpio-reset = <1>;
		/*qcom,gpio-vio = <2>;*/
		qcom,gpio-vdig = <2>;//3
		qcom,gpio-vana = <3>;//4
		qcom,gpio-req-tbl-num = <0 1 2 3>;//01234
		qcom,gpio-req-tbl-flags = <1 0 0 0>;//10000
				qcom,gpio-req-tbl-label = "CAMIF_MCLK0", 
                                                           "CAM_RESET0", 
                                                             "CAM_VDIG", 
                                                              "CAM_VANA";
		qcom,sensor-position = <0>;
		qcom,sensor-mode = <0>;
		qcom,cci-master = <0>;
		status = "ok";
		clocks = <&clock_mmss MCLK0_CLK_SRC>,
			<&clock_mmss MMSS_CAMSS_MCLK0_CLK>;
