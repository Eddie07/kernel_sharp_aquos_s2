				};
			};

			sdm660_gpio21_default: sdm660_gpio21_default {

				mux {
					pins = "gpio21";
					function = "func2";
				};

				config {
					pins = "gpio21";
					drive-strength = <2>;
					bias-disable;
				};
			};

			sdm660_gpio22_default: sdm660_gpio22_default {

				mux {
					pins = "gpio22";
					function = "gpio";
				};

				config {
					pins = "gpio22";
					drive-strength = <2>;
					bias-disable;
					input-enable;
				};
			};

			sdm660_gpio23_default: sdm660_gpio23_default {

				mux {
					pins = "gpio23";
					function = "gpio";
				};

				config {
					pins = "gpio23";
					drive-strength = <2>;
					bias-disable;
				};
			};

			sdm660_gpio24_active: sdm660_gpio24_active {

				mux {
					pins = "gpio24";
					function = "gpio";
				};

				config {
					pins = "gpio24";
					drive-strength = <2>;
					bias-disable;
